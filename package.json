{
  "name": "macgyver-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17"
  },
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev": "yarn start",
    "build": "next build",
    "start": "next dev | pino-pretty",
    "postinstall": "husky install",
    "lint": "next lint"
  },
  "dependencies": {
    "@navikt/aksel-icons": "^4.12.1",
    "@navikt/ds-css": "^4.12.3",
    "@navikt/ds-css-internal": "^3.4.3",
    "@navikt/ds-react-internal": "^3.4.3",
    "@navikt/next-api-proxy": "^3.3.0",
    "@navikt/next-auth-wonderwall": "^1.5.0",
    "@navikt/next-logger": "^1.19.0",
    "next": "13.5.2",
    "pino": "^8.15.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.2.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@navikt/eslint-config-teamsykmelding": "^4.3.0",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "eslint": "8.49.0",
    "eslint-config-next": "^13.5.2",
    "husky": "^8.0.3",
    "msw": "^1.3.1",
    "next-logger": "^3.0.2",
    "node-gyp": "^9.4.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.3",
    "semver": "^7.5.4",
    "typescript": "5.2.2",
    "word-wrap": "^1.2.4"
  },
  "resolutions": {
    "eslint-plugin-react-hooks": "5.0.0-canary-7118f5dd7-20230705"
  },
  "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
  "eslintConfig": {
    "extends": [
      "@navikt/eslint-config-teamsykmelding",
      "next/core-web-vitals"
    ]
  },
  "lint-staged": {
    "*.{md,html,css,json,graphql,graphqls,yml,md,mdx}": "yarn prettier --write",
    "*.{ts,tsx}": "yarn eslint --fix --max-warnings=0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
